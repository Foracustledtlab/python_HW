#HW week4

import pandas as pd
import re

def urls(text):
    url = re.findall(r'http[s]?://+', text)
    return url

H1 = pd.read_excel('C:\\Users\\10643\\Desktop\\programming\\123\\2023-Python-Advanced\\Week 4\\H1.xls')
H1['Hyperlinks'] = [urls(H1.iloc[i,3])[0] for i in range(len(H1.index))]

H1.to_excel('C:\\Users\\10643\\Desktop\\programming\\123\\2023-Python-Advanced\\Week 4\\New_HW1.xlsx')
